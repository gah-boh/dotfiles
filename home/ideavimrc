set surround

let mapleader=" "

nnoremap <Leader>s :action SaveAll<CR>
nnoremap <Leader>d :action CloseEditor<CR>
nnoremap <Leader>o :action GotoFile<CR>
nnoremap <Leader>f :action SelectInProjectView<CR>

" Window Navigation Shortcuts
nnoremap <c-j> <c-w>j
nnoremap <c-k> <c-w>k
nnoremap <c-l> <c-w>l
nnoremap <c-h> <c-w>h

vmap <Leader>y "+y
nmap <Leader><Leader> V

